{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "settings": {
    "import/core-modules": ["aws-sdk"]
  }
}
